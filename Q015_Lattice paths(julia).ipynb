    "function path(x,y)\n",
    "    n=5\n",
    "    if (x == n) && (y == n)\n",
    "        return 1\n",
    "    elseif ((x < n) && (y == n))\n",
    "        return path(x+1,y)\n",
    "    elseif ((y < n) && (x == n))\n",
    "        return path(x,y+1)\n",
    "    else\n",
    "        return path(x+1,y) + path(x,y+1)\n",
    "    end\n",
    "end\n",
    "println(path(0,0))"
   ]
